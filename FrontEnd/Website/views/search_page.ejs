<% include partials/header %>

<div class="user-div" id="user-stats-div">
	<div class="center-div" id="view-title">
		<p class="align-center">
			Searched for: 
		</p>
	</div>
	
	<!-- Loops over all question titles -->
	
	
	<div class="make-flex flex-vert" id="view-results">
		<% for (var search_q_index = 0; search_q_index<search_page.search_question.search_question_list.length; search_q_index++) { %>
			<% var questions_list = search_page.search_question.search_question_list %>
			
			<div class="make-flex flex-vert  view-result">
				<p class="view-result-p">
					<a href=<%= "/question_forum/" + String(questions_list[search_q_index].questionID) %> > 
						<%= questions_list[search_q_index].questionList %>
					</a>
				</p>
				<p class="self-right date-result-p">
					<%= questions_list[search_q_index].userName %> - <%= questions_list[search_q_index].date_ask %>
				</p>
			</div>
			<% } %>
		</div>
		
	</div>
	
	
	<% include partials/footer %>